	$(function () {
	    $('#container1').highcharts({
	        chart: {
	            type: 'column'
	        },
	        title: {
	            text: ''
	        },
	        
	        xAxis: {
	            categories: tag,
	            crosshair: true,
	            title: {
	                text: 'Sensors'
	            }
	        },
	        yAxis: {
	            min: 0,
	            title: {
	                text: 'Revenue(USD)'
	            }
	        },
	        tooltip: {
	            headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
	            pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
	                '<td style="padding:0"><b>{point.y:.1f} USD</b></td></tr>',
	            footerFormat: '</table>',
	            shared: true,
	            useHTML: true
	        },
	        plotOptions: {
	            column: {
	                pointPadding: 0.2,
	                borderWidth: 0
	            }
	        },
	        series: [{
	            name: 'Revenue Per Sensor',
	            data: totalquery

	        }]
	    });
	});
	




</script>

