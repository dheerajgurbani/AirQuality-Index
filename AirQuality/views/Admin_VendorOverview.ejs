 tag1.push(<%= data1[d].vendor_id %>)



 <%}%>

	$(function () {
	    $('#vendorRevenue').highcharts({
	        chart: {
	            type: 'area'
	        },
	        title: {
	            text: ''
	        },
	        
	        xAxis: {
	            categories: tag1,
	            crosshair: true,
	            title: {
	                text: 'Vendors'
	            }
	        },
	        yAxis: {
	            min: 0,
	            title: {
	                text: 'Revenue(USD)'
	            }
	        },
	        tooltip: {
	            headerFormat: '<span style="font-size:10px">{point.key}</span><table>',
	            pointFormat: '<tr><td style="color:{series.color};padding:0">{series.name}: </td>' +
	                '<td style="padding:0"><b>{point.y:.1f} USD</b></td></tr>',
	            footerFormat: '</table>',
	            shared: true,
	            useHTML: true
	        },
	        plotOptions: {
	            column: {
	                pointPadding: 0.2,
	                borderWidth: 0
	            }
	        },
	        series: [{
	            name: 'Revenue(USD) per Vendor',
	            data: totalquery1

	        }]
	    });
	});
	




</script>



