<script type="text/javascript">
var totalquery=[];

var tag=[];

 <% for (var d=0;d<data.length;d++){%>
 totalquery.push(<%= data[d].noOfSensors %>);
 tag.push("<%= data[d].vendor_name %>")
 
/*  tag.push("C#");
